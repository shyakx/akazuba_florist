{
  "redirects": [
    {
      "source": "http://akazubaflorist.com/(.*)",
      "destination": "https://akazubaflorist.com/$1",
      "permanent": true
    },
    {
      "source": "http://www.akazubaflorist.com/(.*)",
      "destination": "https://www.akazubaflorist.com/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}